<h1>Login</h1>

<%= form_with url: login_path, method: :post do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <%= form.submit 'Login' %>
<% end %>

<%= link_to 'Sign Up', signup_path %>
